<p id="notice"><%= notice %></p>

<h1>Listing Forestries</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @forestries.each do |forestry| %>
      <tr>
        <td><%= link_to 'Show', forestry %></td>
        <td><%= link_to 'Edit', edit_forestry_path(forestry) %></td>
        <td><%= link_to 'Destroy', forestry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Forestry', new_forestry_path %>
