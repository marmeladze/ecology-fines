<p id="notice"><%= notice %></p>

<h1>Listing Compensation Rates</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @compensation_rates.each do |compensation_rate| %>
      <tr>
        <td><%= link_to 'Show', compensation_rate %></td>
        <td><%= link_to 'Edit', edit_compensation_rate_path(compensation_rate) %></td>
        <td><%= link_to 'Destroy', compensation_rate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Compensation rate', new_compensation_rate_path %>
